{
  "name": "vk-market",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy:gh": "tsc && vite build --base=/vk-mini-app/ && gh-pages -d dist",
    "dev:vk": "cross-env PORT=10888 HTTPS=true vite --port 10888",
    "tunnel": "vk-tunnel --insecure=1 --http-protocol=https --ws-protocol=wss --host=0.0.0.0 --port=10888 app_id=51878613",
    "deploy:vk": "tsc && vite build && vk-miniapps-deploy",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "check": "tsc && npm run prettier && npm run lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "@vkontakte/icons": "^2.102.0",
    "@vkontakte/vkui": "^6.0.1",
    "axios": "^1.6.7",
    "clsx": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vkontakte/vk-bridge": "^2.14.1",
    "@vkontakte/vk-miniapps-deploy": "^0.1.6",
    "@vkontakte/vk-tunnel": "^0.1.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.2",
    "husky": "^8.0.0"
  }
}
